<table>
  <thead>
    <tr>
      <th>Organization(s)</th>
    </tr>
  </thead>
  <tbody>
    <% if @event.organizations %>
      <% @event.organizations.each do |organization| %>
        <tr>
          <td><%= organization.name %></td>
          <td><%= link_to 'Show', user_organization_path(current_user, organization) %></td>
        </tr>
      <% end %>
    <% else %>
      <div> No organization </div>
    <% end %>
  </tbody>
</table>

