    <% if @organization.events %>
      <% @organization.events.each do |event| %>
        <tr>
          <td><%= event.name %></td>
          <td><%= event.date %></td>
          <td><%= @organization.name %></td>
          <td><%= link_to 'Show Event', organization_event_path(@organization, event) %></td>
        </tr>
      <% end %>
    <% else %>
      <div> No events </div>
    <% end %>


